{
  "version": 2,
  "alias": ["snailjs.brn.sh"],
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/main.js", "dest": "/public/main.js" },
    { "src": "/(.*)", "dest": "/server.js" }
  ],
  "github": {
    "enabled": false
  }
}
